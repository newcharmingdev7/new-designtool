<li class={{this.classNames}} ...attributes>
  <Hds::Interactive
    class="hds-pagination-nav__control hds-typography-body-100 hds-font-weight-medium"
    aria-selected={{if this.isSelected "page" null}}
    {{on "click" this.onClick}}
  >
    <span class="sr-only">page </span>{{@page}}
  </Hds::Interactive>
</li>