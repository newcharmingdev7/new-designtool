<Hds::Interactive
  class={{this.classNames}}
  @current-when={{@current-when}}
  @models={{hds-link-to-models @model @models}}
  @query={{hds-link-to-query @query}}
  @replace={{@replace}}
  @route={{@route}}
  @isRouteExternal={{@isRouteExternal}}
  @href={{@href}}
  target="_blank"
  rel="noopener noreferrer"
  ...attributes
>
  {{yield}}
</Hds::Interactive>