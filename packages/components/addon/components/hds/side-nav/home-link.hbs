<Hds::Interactive
  @href={{@href}}
  @route={{@route}}
  @models={{hds-link-to-models @model @models}}
  @query={{hds-link-to-query @query}}
  @icon={{@icon}}
  @isRouteExternal={{@isRouteExternal}}
  @current-when={{@current-when}}
  class="hds-side-nav__header-logo"
  ...attributes
>
  <FlightIcon class="hds-side-nav__header-logo-icon" @name={{@icon}} @stretched={{true}} />
</Hds::Interactive>