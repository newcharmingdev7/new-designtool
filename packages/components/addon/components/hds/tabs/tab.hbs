<li class={{this.classNames}} role="presentation">
  <a
    role="tab"
    href={{concat "#" this.panelId}}
    id={{this.tabId}}
    {{! Used to label related Panel for a11y }}
    {{! aria-selected="true"  TODO: Set by arg: "isSelected" }}
    {{! tabindex="-1"  TODO: Set if not aria-selected ("isSelected") }}
    ...attributes
    {{did-insert this.didInsertNode}}
  >
    {{yield}}
  </a>
</li>