{{! Use for "previous" & "next" button, add type arg}}
{{! onClick={{this.prevPage }}

<Hds::Interactive
  class="hds-pagination__page-link"
  type={{@type}}
  onClick={{this.changePage}}
  aria-label="Previous page"
  disabled={{@disabled}}
>
  <FlightIcon @name="chevron-left" class="hds-pagination__page-icon" />
  {{#if this.isCompact}}
    <span class="hds-pagination__page-label" aria-hidden="true">
      Previous
    </span>
  {{/if}}
</Hds::Interactive>