{
  "name": "@hashicorp/ember-flight-icons",
  "version": "5.0.3",
  "description": "The Ember addon for the HashiCorp Flight SVG icon set",
  "keywords": [
    "ember-addon",
    "hashicorp",
    "icon",
    "svg",
    "flight"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hashicorp/design-system.git",
    "directory": "packages/ember-flight-icons"
  },
  "license": "MPL-2.0",
  "author": "HashiCorp Design Systems <design-systems@hashicorp.com>",
  "scripts": {
    "build": "rollup --config",
    "prepublishOnly": "rollup --config",
    "start": "rollup --config --watch --environment development",
    "lint": "concurrently \"yarn:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"yarn:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.7",
    "@hashicorp/flight-icons": "^3.3.0",
    "ember-get-config": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/preset-typescript": "^7.24.7",
    "@ember/optional-features": "^2.1.0",
    "@embroider/addon-dev": "^4.3.1",
    "@embroider/test-setup": "^3.0.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@rollup/plugin-babel": "^6.0.4",
    "concurrently": "^8.2.2",
    "ember-template-lint": "^4.18.2",
    "ember-template-lint-plugin-prettier": "^4.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^11.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^8.1.1",
    "prettier": "^2.8.8",
    "rollup": "^4.17.2",
    "rollup-plugin-copy": "^3.5.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./components/flight-icon.js": "./dist/_app_/components/flight-icon.js",
      "./instance-initializers/load-sprite.js": "./dist/_app_/instance-initializers/load-sprite.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "engines": {
    "node": "16.* || >= 18"
  }
}
