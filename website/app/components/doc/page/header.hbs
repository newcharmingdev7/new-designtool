<header class="doc-page-header" ...attributes>
  <LinkTo @route="index" class="doc-page-header__logo" aria-label="home page">
    <Doc::HdsLogo alt="HashiCorp Design System" />
  </LinkTo>
  <nav class="doc-page-header__menu" aria-label="primary navigation">
    <ul>
      {{#each this.menu as |item|}}
      <pre>{{item.isCurrent}}</pre>
        <li class={{if item.isCurrent 'doc-page-header__menu-item--is-current'}}><LinkTo @route={{item.route}} class="aaa {{if item.isCurrent 'doc-page-header__menu-item--is-current'}}">{{item.label}}</LinkTo></li>
      {{/each}}
    </ul>
  </nav>
  <button type="button" {{on "click" this.test}}>ğŸš§</button>
</header>