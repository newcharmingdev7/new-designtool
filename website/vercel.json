{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "all"
        }
      ]
    },
    {
      "source": "/testing(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "none"
        }
      ]
    }
  ],
  "framework": "ember",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/error.html" }
  ]
}
